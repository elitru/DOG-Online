@import './../../../../styles.styl'

#info-box
    width 100%
    height 100%
    padding 20px
    background white
    border solid 6px blue_dark
    border-radius 16px
    display grid
    place-items center

    p
        font-size 20px
        margin 0px
        padding 0px
        color blue_dark
        font-weight bold
        width 100%
        text-align left

        i
            padding-right 8px